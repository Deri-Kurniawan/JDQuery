{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "linebreak-style": "off",
      "no-unused-vars": "off",
      "consistent-return": "off",
      "no-underscore-dangle": "off"
    }
}
